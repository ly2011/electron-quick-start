{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "app/main.js",
  "scripts": {
    "dev": "npm-run-all -p hot-electron hot-react-dev",
    "hot-electron": "electron -r babel-register ./app/main.js",
    "hot-react-dev": "webpack-dev-server --config build/webpack.config.js --history-api-fallback --colors --inline --hot --content-base app",
    "start": "cross-env HOT=1 NODE_ENV=production electron .",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register .",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": ["Electron", "quick", "start", "tutorial", "demo"],
  "author": "ly2011",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "devtron": "^1.4.0",
    "electron": "~1.6.2",
    "electron-debug": "^1.4.0",
    "electron-is-dev": "^0.3.0",
    "electron-packager": "^8.7.2",
    "electron-window-state": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.3.1",
    "lint-staged": "^4.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "typed-css-modules": "^0.3.0",
    "typed-css-modules-loader": "^0.0.10",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "config": "^1.26.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
